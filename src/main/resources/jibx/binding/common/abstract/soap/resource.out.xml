  <!-- none -->

  <!-- ABSTRACT MAPPINGS -->
  <!-- identifier -->
  <mapping class="de.escidoc.core.resources.Resource$Identifier"
    abstract="true">
    
    <value name="objid" style="attribute" set-method="setObjid"
      get-method="getObjid" usage="optional"/>
